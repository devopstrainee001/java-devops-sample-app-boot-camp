       agent any
       stages {
              stage('First stage') {
                     steps {
                            echo 'This is first stage'
                     }
              }
             stage('Second stage') {
                    steps {
                           echo 'This is Second stage'
                    }
              }
             stage('Third stage') {
                    steps {
                           echo 'This is Third stage'
                    }
              }
        }
   }



